<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
<!--
    stosowane w projektach starych, raczej stosuje się konfiguracje beanów w klasach konfiguracyjnych
    ala SubtractionOperationConfiguration w przypadku zastosowania nietypowych konstruktorów
    lub oznaczeniem klasy annotacja @Bean w przypadku klas bezkonstruktorowych
    lub tez takich parametrów które możemy dostarczyć z komponentów springa
 -->
    <bean id="multiplicationOperation" class="pl.sda.spring.operation.MultiplicationOperation"/>
    <bean id="powerOperation" class="pl.sda.spring.operation.PowerOperation"/>
</beans>